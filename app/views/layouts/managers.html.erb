<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'managers', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'managers', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% content_for :content do %>  
      <%= yield %>
    <% end %>
    <%= render template: "layouts/application" %>
  </body>
</html>
